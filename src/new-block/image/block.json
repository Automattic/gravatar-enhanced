{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gravatar/block-image",
	"version": "0.1.0",
	"title": "Image",
    "parent": [],
	"attributes": {
		"linkUrl": {
			"type": "string"
		},
		"imageUrl": {
            "type": "string"
        },
        "imageWidth": {
            "type": "number"
        },
        "imageHeight": {
            "type": "number"
        },
		"imageAlt": {
            "type": "string"
        },
		"className": {
			"type": "string"
		}
	},
	"supports": {
        "html": false,
		"inserter": false
    },
	"textdomain": "gravatar-enhanced",
    "editorScript": "file:../../block-image.js"
}
